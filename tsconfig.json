{
  "compilerOptions": {
	"target":"es2017",
	"module":"esnext",
	"strict":true,
	"outDir":"lib/",
	"esModuleInterop":true,
	"skipLibCheck":true,
	"declaration":true,
	"forceConsistentCasingInFileNames":true,
	"resolveJsonModule":true,
	"moduleResolution":"node",
	"allowSyntheticDefaultImports":true
  },
  "include": ["src/**/*.ts"]
}
